@using WebAppMVCprejoinerB2.Models
@model Cal
@{
    ViewData["Title"] = "Add";
}

<h1>Add</h1>


<form method="post">
    <table>
        <tr>
            <th>Enter X:</th>
            <td><input type="number" asp-for="x" /></td>
        </tr>

        <tr>
            <th>Enter y:</th>
            <td><input type="number" asp-for="y" /></td>
        </tr>

        <tr>
            <td>
                <input type="submit" name="submit" value="submit" />
            </td>

            <td>
                Add is : @TempData["res"]
            </td>
        </tr>

    </table>
</form>