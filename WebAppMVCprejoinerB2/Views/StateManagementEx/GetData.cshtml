@using WebAppMVCprejoinerB2.Controllers

@{
    ViewData["Title"] = "GetData";
}

<h1>GetData</h1>

<table class="table">
    <thead>
        <tr>
            <th>
               ID
            </th>
            <th>
               Name
            </th>
       
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewData["res1"] as IList<MyData>)
        {
            <tr>
                <td>
                   @item.ID

                </td>
                <td>
                   @item.Name
                </td>
               
            </tr>
        }
    </tbody>
</table>
